<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Memefy.MenuPage">

    <StackLayout Margin="20" Orientation="Vertical" >
        <Image x:Name="image" VerticalOptions="CenterAndExpand" HorizontalOptions="Center" Aspect="AspectFit"/>
        <Button Text="Memefy!!" Clicked="MemefyPhoto" VerticalOptions="End"/>
        <Button Text="Manage Memes" Clicked="ShowList" VerticalOptions="End"/>
    </StackLayout>
    
</ContentPage>